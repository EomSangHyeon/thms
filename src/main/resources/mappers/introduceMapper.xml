<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.thms.mapper.IntroduceMapper">
	<select id="subjectList" resultType="SubjectVO">
		<![CDATA[
		select `sjid`, `sjname` from
		`tbl_subject`
		where `sjid`>0
		]]>
	</select>
	
	<select id="doctorList" resultType="doctorVO">
		<![CDATA[
		select * from 
		tbl_doctor		
		]]>
	</select>
</mapper> 
